{
  "name": "insurance-llm-system",
  "version": "1.0.0",
  "description": "AI-powered insurance analysis system with React frontend and FastAPI backend",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "frontend": "cd frontend && npm start",
    "backend": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "install-all": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "build": "cd frontend && npm run build",
    "test": "cd frontend && npm test",
    "backend-install": "cd backend && pip install -r requirements.txt",
    "frontend-install": "cd frontend && npm install"
  },
  "keywords": [
    "insurance",
    "ai",
    "llm",
    "react",
    "fastapi",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.2",
    "react-syntax-highlighter": "^15.5.0"
  }
}
